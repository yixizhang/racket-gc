#! /bin/bash
rm basic/*.rktd 2> /dev/null
rm basic/*.pdf 2> /dev/null
for coll in generational hybrid;
do
    cp ../collectors/$coll.rkt collector.rkt 2> /dev/null
    for cache in `ls ../caches/`;
    do
        cp ../caches/$cache cache.rkt 2> /dev/null
        for test in `ls basic/*.rkt`;
        do
            echo "$coll & $cache -> $test"
            racket runner.rkt $coll $test
        done
    done
done
#racket plot-graphs.rkt
